# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-actual-project-id
GOOGLE_APPLICATION_CREDENTIALS=./Credentials/google-cloud-key.json
VERTEX_AI_LOCATION=us-central1

# Elastic Cloud Configuration (Hosted Deployment)
# Get these from your Elastic Cloud deployment:
# 1. Cloud ID: Copy from deployment overview page
# 2. API Key: Create in Kibana > Stack Management > Security > API Keys
ELASTIC_CLOUD_ID=your-cloud-id-here
ELASTIC_API_KEY=your-api-key-here

# GitHub Configuration (Optional for now)
GITHUB_TOKEN=your-github-token
GITHUB_WEBHOOK_SECRET=your-webhook-secret

# Application Configuration
ENVIRONMENT=development
DEBUG=True
CORS_ORIGINS=http://localhost:3000,https://your-frontend-domain.com

# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=Hackathon Agent API

# AI Model Configuration (Gemini Pro + Embeddings)
GEMINI_MODEL=gemini-pro
EMBEDDING_MODEL=textembedding-gecko@003
MAX_TOKENS=2048
TEMPERATURE=0.7

# Elasticsearch Indices
DEVPOST_INDEX=devpost_projects
DOCUMENTATION_INDEX=hackathon_docs
GITHUB_INDEX=github_activity